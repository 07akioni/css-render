{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.88.0",
  "pnpmVersion": "7.19.0",
  "pnpmOptions": {
    "useWorkspaces": true
  },
  "nodeSupportedVersionRange": ">=14.15.0 <17.3.0 || >= 18.0.0 < 19.0.0",
  "ensureConsistentVersions": true,
  "projectFolderMinDepth": 2,
  "projectFolderMaxDepth": 2,
  "gitPolicy": {},
  "repository": {
    "url": "git@github.com:07akioni/css-render.git",
    "defaultBranch": "master"
  },
  "projects": [
    {
      "packageName": "css-render",
      "projectFolder": "packages/css-render",
      "shouldPublish": true,
      "versionPolicyName": "css-render"
    },
    {
      "packageName": "@css-render/vue3-ssr",
      "projectFolder": "packages/vue3-ssr",
      "shouldPublish": true,
      "versionPolicyName": "css-render"
    },
    {
      "packageName": "@css-render/plugin-bem",
      "projectFolder": "packages/plugin-bem",
      "shouldPublish": true,
      "versionPolicyName": "css-render"
    },
    {
      "packageName": "@css-render/test-shared",
      "projectFolder": "packages/test-shared",
      "shouldPublish": false
    },
    {
      "packageName": "@css-render/eslint-config",
      "projectFolder": "packages/eslint-config",
      "shouldPublish": false
    },
    {
      "packageName": "@css-render/docs",
      "projectFolder": "packages/docs",
      "shouldPublish": false
    }
  ]
}
